{
  "name": "neuromark",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:mac": "VITE_TARGET=mac vite build && rsync -a --delete dist/ macos/Resources/dist/",
    "build:mac:app": "npm run build:mac && xcodebuild -project macos/NeuroMarkMac.xcodeproj -scheme NeuroMarkMac -configuration Debug -derivedDataPath macos/DerivedData build",
    "build:mac:one": "bash scripts/macos-build.sh",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "@supabase/supabase-js": "^2.90.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
